{
  "expo": {
    "name": "crechespots",
    "slug": "crechespots",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "crechespots",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.crechespots",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "crechespots"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.crechespots",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "crechespots",
              "host": "expo.io"
            }
          ],
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "99a507e3-948f-434d-b446-3d456fd0aaab"
      }
    }
  }
}
